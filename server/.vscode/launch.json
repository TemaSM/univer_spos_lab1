{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Server : Debug",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "run",
        "debug"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server : Release",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "run",
        "release"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
